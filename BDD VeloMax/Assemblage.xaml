<Window x:Class="BDD_VeloMax.Assemblage"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:BDD_VeloMax"
        mc:Ignorable="d"
        Title="Assemblage" Height="450" Width="800">
    <Grid>
        <DataGrid Margin="164,0,0,144" Name ="tab"></DataGrid>
        <ComboBox x:Name="ComboBoxAssemblage" HorizontalAlignment="Left" Margin="10,43,0,0" VerticalAlignment="Top" Width="149" Loaded="ComboBoxAssemblage_Loaded"/>
        <Label x:Name="AssemblageLabel" Content="Liste n° :" HorizontalAlignment="Left" Margin="10,12,0,0" VerticalAlignment="Top" Width="149"/>
        <Button x:Name="ButtonValider" Content="Valider" HorizontalAlignment="Left" Margin="47,82,0,0" VerticalAlignment="Top" Width="75" Click="ButtonValider_Click"/>
        <Button x:Name="ButtonImpression" Content="Imprimer" HorizontalAlignment="Left" Margin="36,205,0,0" VerticalAlignment="Top" Width="101" Height="25" Click="ButtonImpression_Click"/>
    </Grid>
</Window>
